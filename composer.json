{
  "name": "your-vendor/your-project",
  "description": "A basic Symfony project",
  "type": "project",
  "require": {
    "php": "^8.1",
    "symfony/framework-bundle": "^6.3",
    "symfony/console": "^6.3",
    "symfony/dotenv": "^6.3",
    "symfony/yaml": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.1.0"
    }
  }
}
